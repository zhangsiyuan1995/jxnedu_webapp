n          </van-col>\r\n          <van-col span=\"11\">\r\n            <div class=\"feeRed\">\r\n              <p>扣款(元)</p>\r\n              <img src=\"../assets/img/chart.png\" alt />\r\n              <h2>+598</h2>\r\n              <p>本月费用</p>\r\n            </div>\r\n          </van-col>\r\n        </van-row>\r\n        <h3>本月课时费共28项组成</h3>\r\n        <div class=\"feeComp\">\r\n          <h2>课时费组成</h2>\r\n          <van-cell v-for=\"(item,index) in feeComp\" :key=\"index\" :value=\"item.money\">\r\n            <template slot=\"title\">\r\n              <span class=\"custom-title\">{{item.classN}}</span>\r\n            </template>\r\n            <template slot=\"label\">{{item.time}}</template>\r\n            <template slot=\"icon\" v-if=\"item.money>0\">\r\n              <van-image :src=\"imgIn\"></van-image>\r\n            </template>\r\n            <template slot=\"icon\" v-if=\"item.money<0\">\r\n              <van-image :src=\"imgOut\"></van-image>\r\n            </template>\r\n          </van-cell>\r\n        </div>\r\n      </van-col>\r\n    </van-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Vue from \"vue\";\r\nimport { Row, Col, Cell, CellGroup, Image, Icon, Divider } from \"vant\";\r\nVue.use(Row)\r\n  .use(Col)\r\n  .use(Cell)\r\n  .use(Image)\r\n  .use(Icon)\r\n  .use(Divider)\r\n  .use(CellGroup);\r\nexport default {\r\n  name: \"personal\",\r\n  data() {\r\n    return {\r\n      imgOut: require(\"../assets/img/feeOut.png\"),\r\n      imgIn: require(\"../assets/img/feeIn.png\"),\r\n      income: \"\",\r\n      deduction: \"\",\r\n      feeComp: [\r\n        {\r\n          classN: \"19寒假点睛班\",\r\n          time: \"2020-01-12\",\r\n          money: -18\r\n        },\r\n        {\r\n          classN: \"19寒假点睛班\",\r\n          time: \"2020-01-12\",\r\n          money: 18\r\n        },\r\n        {\r\n          classN: \"19寒假点睛班\",\r\n          time: \"2020-01-12\",\r\n          money: 18\r\n        },\r\n        {\r\n          classN: \"19寒假点睛班\",\r\n          time: \"2020-01-12\",\r\n          money: -18\r\n        }\r\n      ]\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style>\r\n.feeBlue {\r\n  background: linear-gradient(to right, #4e84f1, #4c69f6);\r\n  padding: 15px 10px;\r\n  color: #ffffff;\r\n  margin-top: 15px;\r\n  border-radius: 5px;\r\n  box-shadow: 1px 2px 1px 1px #c9dbff;\r\n}\r\n.feeRed {\r\n  background: linear-gradient(to right, #f2564b, #f83b3a);\r\n  padding: 15px 10px;\r\n  color: #ffffff;\r\n  margin-top: 15px;\r\n  border-radius: 5px;\r\n  box-shadow: 1px 2px 1px 1px #ffd4d1;\r\n}\r\n.feeBlue :first-child {\r\n  display: inline-block;\r\n  margin: 0;\r\n  font-size: 10px;\r\n}\r\n.feeBlue img {\r\n  display: inline-block;\r\n  width: 30px;\r\n  vertical-align: top;\r\n  float: right;\r\n}\r\n.feeBlue p:nth-child(4) {\r\n  text-align: right;\r\n  margin-bottom: 0;\r\n  font-size: 10px;\r\n}\r\n\r\n.feeRed :first-child {\r\n  display: inline-block;\r\n  margin: 0;\r\n  font-size: 10px;\r\n}\r\n.feeRed img {\r\n  display: inline-block;\r\n  width: 30px;\r\n  vertical-align: top;\r\n  float: right;\r\n}\r\n.feeRed p:nth-child(4) {\r\n  text-align: right;\r\n  margin-bottom: 0;\r\n  font-size: 10px;\r\n}\r\n.feeBlue h2,\r\n.feeRed h2 {\r\n  font-weight: 150;\r\n}\r\n.feeComp {\r\n  background-color: white;\r\n  padding: 15px 10px 15px 0;\r\n  border-radius: 5px;\r\n  box-shadow: 1px 1px 1px 1px #f5f0f0;\r\n}\r\n.feeComp .van-cell__value {\r\n  font-size: 18px;\r\n  margin-top: 10px;\r\n}\r\n.feeComp img {\r\n  width: 40px;\r\n  height: 40px;\r\n  vertical-align: top;\r\n  margin-right: 5px;\r\n}\r\n.feeComp h2 {\r\n  margin-left: 15px;\r\n  font-weight: 350;\r\n}\r\n</style>\r\n"]}]}